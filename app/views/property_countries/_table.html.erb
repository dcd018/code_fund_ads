<div class="table-responsive">
  <table class="js-datatable table table-striped table-compact table-borderless u-datatable u-datatable__striped u-datatable__trigger">
    <thead>
      <tr>
        <th class="text-center">Country</th>
        <th class="text-center"><span class="d-block small text-muted">estimated</span>Earnings</th>
        <th class="text-center"><span class="d-block small text-muted">total</span>Impressions</th>
        <th class="text-center"><span class="d-block small text-muted">Paid</span> Percentage</th>
        <th class="text-center"><span class="d-block small text-muted">Paid</span> Impressions</th>
        <th class="text-center"><span class="d-block small text-muted">Paid</span> Clicks</th>
        <th class="text-center"><span class="d-block small text-muted">Fallback</span> Percentage</th>
        <th class="text-center"><span class="d-block small text-muted">Fallback</span> Impressions</th>
        <th class="text-center"><span class="d-block small text-muted">Fallback</span> Clicks</th>
      </tr>
    </thead>
    <tbody>
      <% @countries.each do |country| %>
      <% end %>
    </tbody>
  </table>
</div>
